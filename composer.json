{
    "name": "sytou/test",
    "description": "First Plugin",
    "type": "library",
    "license": "MIT",
    "minimum-stability": "dev",
    "require": {
        "php": ">=8.1"
    },
    "autoload": {
        "psr-4": {
            "sytou\\test\\": "src"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "sytou\\test\\TestServiceProvider"
            ]
        },
        "post-install-cmd": [
            "@php artisan migrate --path=vendor/sytou/test/src/Migrations"
        ],
        "post-update-cmd": [
            "@php artisan migrate --path=vendor/sytou/test/src/Migrations"
        ]
    }
}
